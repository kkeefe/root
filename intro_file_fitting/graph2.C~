//this is the second example of a graphing function
//going to use a random number generator

{
  g2 = new TGraph();
  g2->SetMarkerStyle(24);
  g2->SetName("First macro Data Fit");
  g2->SetMarkerColor(kBlue);
  
  for(int i = 0; i < 10; i++){
    g2->SetPoint(i, i, i*i*i);
  }

  g2->Draw("ALP");
  
}
